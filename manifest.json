{
  "manifest_version": 3,
  "name": "Image URL Extractor",
  "version": "1.0",
  "description": "Extracts image URLs from the current page",
  "permissions": ["activeTab", "scripting"],
  "action": {
      "default_popup": "popup.html"
  },
  "background": {
      "service_worker": "background.js"
  }
}